    x:Key="NullToVisible" />

  <converter:PossibleNullValueToSecondInputBinding
    x:Key="PossibleNullValueToSecondInputBinding" />

  <converter:SingleObjectToArray
    x:Key="SingleObjectToArray" />

  <converter:SpecificStringToVisibility
    x:Key="SpecificStringToVisibility" />

  <converter:StringNullOrEmptyToVisibility
    x:Key="StringNullOrEmptyToVisibility" />

  <converter:StringToResource
    x:Key="StringToResource" />

  <converter:SumOfTimeSpans
    x:Key="SumOfTimeSpans" />

  <converter:TypeNameToResource
    x:Key="TypeNameToResource" />

  <converter:PinnedToThicknessConverter
    x:Key="PinnedToThickness" />

  <converters:ThicknessToDoubleConverter
    x:Key="ThicknessToDouble" />
  
  <converter:PercentToDoubleConverter
    x:Key="PercentToDouble" />
</ResourceDictionary>