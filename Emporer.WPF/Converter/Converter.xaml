<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:converter="clr-namespace:Emporer.WPF.Converter"
  xmlns:converters="http://metro.mahapps.com/winfx/xaml/shared">

  <converter:AbbreviatedDay
    x:Key="AbbreviatedDay" />

  <converter:BoolToVisibilityReversable
    x:Key="BoolToVisibilityReversable" />

  <converter:EmptyToVisibleConverter
    x:Key="EmptyToVisible" />

  <BooleanToVisibilityConverter
    x:Key="BoolToVisibility" />

  <converter:EnumMatchToBoolean
    x:Key="EnumMatchToBoolean" />

  <converter:HasItems
    x:Key="HasItems" />

  <converter:HighlightTextBlock
    x:Key="HighlightTextBlock" />

  <converter:InvertBool
